	shortjournal = {Vaccine},
	author = {Burnett, Eleanor M. and Francois, Jeannot and Sreenivasan, Nandini and Wannemuehler, Kathleen and Faye, Papa Coumba and Tohme, Rania A. and Delly, Patrick and Deslouches, Yves Gaston and Etheart, Melissa D. and Dismer, Amber Marie and Patel, Roopal and Date, Kashmira},
	urldate = {2019-09-18},
