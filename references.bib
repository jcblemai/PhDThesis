@article{Matrajt:VaccineOptimizationCOVID19:2020a,
  title = {Vaccine Optimization for {{COVID}}-19: {{Who}} to Vaccinate First?},
  shorttitle = {Vaccine Optimization for {{COVID}}-19},
  author = {Matrajt, Laura and Eaton, Julia and Leung, Tiffany and Brown, Elizabeth R.},
  date = {2020-02-01},
  journaltitle = {Science Advances},
  volume = {7},
  number = {6},
  eprint = {33536223},
