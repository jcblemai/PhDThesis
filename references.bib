@article{Faruque:SeasonalEpidemicsCholera:2005,
  title = {Seasonal Epidemics of Cholera Inversely Correlate with the Prevalence of Environmental Cholera Phages},
  author = {Faruque, Shah M. and Naser, Iftekhar Bin and Islam, M. Johirul and Faruque, A. S. G. and Ghosh, A. N. and Nair, G. Balakrish and Sack, David A. and Mekalanos, John J.},
  date = {2005-02-01},
