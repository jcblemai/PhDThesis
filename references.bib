@article{Escobar:GlobalMapSuitability:2015,
  title = {A Global Map of Suitability for Coastal {{Vibrio}} Cholerae under Current and Future Climate Conditions},
  author = {Escobar, Luis E. and Ryan, Sadie J. and Stewart-Ibarra, Anna M. and Finkelstein, Julia L. and King, Christine A. and Qiao, Huijie and Polhemus, Mark E.},
