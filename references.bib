@article{Gaudart:SpatioTemporalDynamicsCholera:2013,
  title = {Spatio-{{Temporal Dynamics}} of {{Cholera}} during the {{First Year}} of the {{Epidemic}} in {{Haiti}}},
  author = {Gaudart, Jean and Rebaudet, Stanislas and Barrais, Robert and Boncy, Jacques and Faucher, Benoit and Piarroux, Martine and Magloire, Roc and Thimothe, Gabriel and Piarroux, Renaud},
  date = {2013-04-04},
