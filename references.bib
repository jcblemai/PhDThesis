	number = {7},
	journaltitle = {Emerging Infectious Diseases},
	shortjournal = {Emerg Infect Dis},
	author = {Piarroux, Renaud and Barrais, Robert and Faucher, Benoît and Haus, Rachel and Piarroux, Martine and Gaudart, Jean and Magloire, Roc and Raoult, Didier},
