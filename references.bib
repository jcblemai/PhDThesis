  file = {/Users/chadi/Zotero/storage/H4YVULDQ/Giordano et al. - 2021 - Modeling vaccination rollouts, SARS-CoV-2 variants.pdf;/Users/chadi/Zotero/storage/893HEKCE/s41591-021-01334-5.html}
