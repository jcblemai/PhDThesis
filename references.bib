  keywords = {Antibodies,Cholera,Diarrhea,El Tor,Immune response,Immunity,Infectious disease immunology,Vibrio cholerae},
  file = {/Users/chadi/Zotero/storage/DEAKMXXS/Leung and Matrajt - 2021 - Protection afforded by previous Vibrio cholerae in.pdf;/Users/chadi/Zotero/storage/L274D886/article.html}
