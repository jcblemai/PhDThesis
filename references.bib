@article{Vezzulli:ClimateInfluenceVibrio:2016,
  title = {Climate Influence on {{Vibrio}} and Associated Human Diseases during the Past Half-Century in the Coastal {{North Atlantic}}},
  author = {Vezzulli, Luigi and Grande, Chiara and Reid, Philip C. and Hélaouët, Pierre and Edwards, Martin and Höfle, Manfred G. and Brettar, Ingrid and Colwell, Rita R. and Pruzzo, Carla},
  date = {2016-08-23},
  journaltitle = {Proceedings of the National Academy of Sciences},
