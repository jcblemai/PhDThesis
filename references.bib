	pmcid = {PMC4872293},
	keywords = {Adolescent, Adult, Antibodies, Bacterial, Cholera, Cholera Vaccines, Female, Humans, Male, Middle Aged, Vibrio cholerae O1, Young Adult, challenge, cholera, efficacy, vaccine, volunteer},
