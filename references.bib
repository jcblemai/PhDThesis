@article{Branas:FlatteningCurveIt:2020,
  title = {Flattening the Curve before It Flattens Us: Hospital Critical Care Capacity Limits and Mortality from Novel Coronavirus ({{SARS}}-{{CoV2}}) Cases in {{US}} Counties},
  shorttitle = {Flattening the Curve before It Flattens Us},
  author = {Branas, Charles C. and Rundle, Andrew and Pei, Sen and Yang, Wan and Carr, Brendan G. and Sims, Sarah and Zebrowski, Alexis and Doorley, Ronan and Schluger, Neil and Quinn, James W. and Shaman, Jeffrey},
  date = {2020-04-06},
