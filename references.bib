  author = {Giordano, Giulia and Colaneri, Marta and Di Filippo, Alessandro and Blanchini, Franco and Bolzern, Paolo and De Nicolao, Giuseppe and Sacchi, Paolo and Colaneri, Patrizio and Bruno, Raffaele},
