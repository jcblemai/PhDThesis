@article{Nishiura:EarlyEpidemiologicalAssessment:2009,
  title = {Early {{Epidemiological Assessment}} of the {{Virulence}} of {{Emerging Infectious Diseases}}: {{A Case Study}} of an {{Influenza Pandemic}}},
  shorttitle = {Early {{Epidemiological Assessment}} of the {{Virulence}} of {{Emerging Infectious Diseases}}},
  author = {Nishiura, Hiroshi and Klinkenberg, Don and Roberts, Mick and Heesterbeek, Johan A. P.},
